select distinct ?iri where
{
    {
        ?S ?iri ?O .
        filter(?iri != dcterms:abstract)
        filter(?iri != dcterms:title)
        filter(?iri != rdf:type)
        filter(?iri != cito:isDiscussedBy)
    }
    union
    {
        ?S rdf:type ?iri .
        filter(?iri != obo:SO_0000417)
    }
}
